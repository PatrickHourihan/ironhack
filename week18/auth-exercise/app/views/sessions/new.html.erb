<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Document</title>
</head>
<body>
	<h1>Login</h1>
		<%= form_tag '/login' do %>
			<%= label_tag "Email" %>
			<%= text_field_tag :email %>

  			<%= label_tag "Password" %>
  			<%= password_field_tag :password %>

  			<%= submit_tag "Login" %>
		<% end %>
</body>
</html>